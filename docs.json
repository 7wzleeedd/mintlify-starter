{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "bajo docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.jpeg",
  "navigation": {
    "tabs": [
      {
        "tab": "docs",
        "groups": [
          {
            "group": "start",
            "pages": [
              "index",
              "ticket",
              ""
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
    "global": {
      "anchors": [
        {
          "anchor": "documentation",
          "href": "https://docs.bojo.wtf",
          "icon": "book-open-cover"
        },
        {
          "anchor": "community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        }
      ]
    }
  },
  "logo": {
    "light": "/logos/light.png",
    "dark": "/logos/light.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
